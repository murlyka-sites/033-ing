					.ui-lang-switcher(:class="{'ui-lang-switcher_open': langSwitcherOpen }")
						.ui-lang-switcher__current(@click="langSwitcherOpen = !langSwitcherOpen")
							| {{ lang.labels[ lang.current ]}}
							button.ui-lang-switcher__corner.button-with-icon()
								svg.button-with-icon__svg(width="13" height="8")
									path(d="M 1 1 7 7 13 1" stroke="white" stroke-width="2" fill="none")
						.ui-lang-switcher__list(:class="{'hidden': !langSwitcherOpen }")
							.ui-lang-switcher__item(:class="{'ui-lang-switcher__item_current': lang.current == 'eng'}" @click="lang.current = 'eng'; langSwitcherOpen = false;") Eng
							.ui-lang-switcher__item(:class="{'ui-lang-switcher__item_current': lang.current == 'rus'}" @click="lang.current = 'rus'; langSwitcherOpen = false;") Рус
							.ui-lang-switcher__item(:class="{'ui-lang-switcher__item_current': lang.current == 'ukr'}" @click="lang.current = 'ukr'; langSwitcherOpen = false;") Укр
					
